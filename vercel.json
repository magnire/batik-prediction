{
  "version": 2,
  "builds": [
    { "src": "predict.py", "use": "@vercel/python" },
    { "src": "*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/predict", "dest": "/predict.py" }
  ],
    "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  }
}
